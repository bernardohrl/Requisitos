<%= bootstrap_form_for(@ocorrencia) do |f| %>
  <%= f.date_field :data %>
  <%= f.time_field :hora %>
  <%= f.text_area :ocorrido %>
  <%= f.text_area :procedimento %>
  <%= f.text_area :sintomaApresentado, label: "Sintoma Apresentado" %>
  <%= f.check_box :paisInformados, label: "Pais foram Informados?" %>
  <%= f.submit "Criar Ocorrencia" %>
<% end %>
