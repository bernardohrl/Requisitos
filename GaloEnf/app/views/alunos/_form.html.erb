<%= bootstrap_form_for(@aluno) do |f| %>
  <%= f.text_field :nome , placeholder: "Nome do Aluno"%>
  <%= f.date_field :dataNascimento %>
  <%= f.text_field :turma , placeholder: "MA-XX"%>
  <%= f.text_field :responsavelNome1, label: "Nome do Responsavel 1", placeholder: "Nome dos pais ou responsaveis"%>
  <%= f.telephone_field :responsavelTelefone1, label: "Telefone do Responsavel 1", placeholder: "(00)9-9999-9999"%>
  <%= f.text_field :responsavelNome2, label: "Nome do Responsavel 2", placeholder: "Nome dos pais ou responsaveis"%>
  <%= f.telephone_field :responsavelTelefone2, label: "Telefone do Responsavel 2", placeholder: "(00)9-9999-9999"%>
  <%= f.submit "Criar Aluno" %>
<% end %>
